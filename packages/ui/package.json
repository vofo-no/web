{
  "version": "0.1.0",
  "name": "@vofo-no/ui",
  "exports": {
    ".": {
      "require": "./dist/design.cjs",
      "default": "./dist/design.modern.js"
    },
    "./tailwind.css": "./dist/tailwind.css",
    "./tailwind-base.js": "./dist/tailwind-base.js"
  },
  "main": "./dist/design.cjs",
  "module": "./dist/design.module.js",
  "unpkg": "./dist/design.umd.js",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "microbundle src/index.ts --jsx React.createElement && yarn build:tw-stylesheet && yarn build:tw-preset",
    "build:tw-stylesheet": "cross-env NODE_ENV=production npx tailwindcss -i ./src/styles/global.css -o ./dist/tailwind.css --minify",
    "build:tw-preset": "node scripts/copy-tailwind-base.js",
    "lint": "eslint src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint"
    }
  },
  "author": "Mats Grimsgaard",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/node": "^17.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "microbundle": "^0.14.2",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
